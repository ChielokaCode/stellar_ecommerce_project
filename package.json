{
  "name": "soroban-localfoodstore-dapp-boilerplate",
  "private": true,
  "version": "2.0.0",
  "babelMacros": {
    "twin": {
      "preset": "emotion"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "lint": "prettier . --check && eslint",
    "lint:fix": "prettier . --write && eslint --fix",
    "lint:format": "prettier . --write",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "setup": "./initialize.sh ${NETWORK:-testnet}",
    "clean": "rm -rf .next soroban",
    "reset": "npm run clean && npm run setup",
    "build:contracts": "./target/bin/soroban contract build",
    "bindings:localfoodstore": "./target/bin/soroban contract bindings typescript --wasm ./target/wasm32-unknown-unknown/release/stellar_localfoodstore_contract.wasm --id $(cat ./dvillaLocalFoodStore/localfoodstore_id) --output-dir ./dvillaLocalFoodStore/localfoodstore-contract --network $(cat ./dvillaLocalFoodStore/network) --overwrite",
    "bindings": "npm run bindings:localfoodstore",
    "preinstall": "npm run build && npm run build:contracts && npm run bindings"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.1",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@soroban-react/chains": "^9.1.8",
    "@soroban-react/contracts": "^9.1.9",
    "@soroban-react/core": "^9.1.9",
    "@soroban-react/freighter": "^9.1.8",
    "@soroban-react/hana": "^9.1.8",
    "@soroban-react/lobstr": "^9.1.8",
    "@soroban-react/types": "^9.1.8",
    "@soroban-react/utils": "^9.1.8",
    "@soroban-react/wallet-data": "9.1.9",
    "@soroban-react/xbull": "^9.1.8",
    "@stellar/freighter-api": "^2.0.0",
    "axios": "^1.7.3",
    "bootstrap": "^5.3.3",
    "csstype": "^3.0.10",
    "dotenv": "^16.4.5",
    "dvilla-token": "file:./dvillaLocalFoodStore/dvilla-token",
    "framer-motion": "^10.16.4",
    "humanize-duration": "^3.32.1",
    "localfoodstore-contract": "file:./dvillaLocalFoodStore/localfoodstore-contract",
    "mdb-react-ui-kit": "^8.0.0",
    "moment": "^2.30.1",
    "next": "^13.1.1",
    "next-seo": "^6.1.0",
    "next-share": "^0.27.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.4",
    "react-hook-form": "^7.52.2",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "sharp": "^0.32.6",
    "soroban-client": "^1.0.1",
    "spinners-react": "^1.0.7",
    "stellar-sdk": "^12.2.0",
    "use-async-effect": "^2.2.7",
    "vercel": "^36.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@emotion/babel-plugin": "^11.11.0",
    "@testing-library/react": "^14.0.0",
    "@types/downloadjs": "^1.4.5",
    "@types/humanize-duration": "^3.27.4",
    "@types/node": "^18.18.7",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-macros": "^3.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-next": "^13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "prettier-plugin-twin.macro": "^1.0.14",
    "tailwindcss": "^3.3.5",
    "twin.macro": "^3.4.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  }
}
